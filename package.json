{
  "name": "public-react-ui",
  "version": "1.0.0",
  "description": "React components that follow Jam3 standards",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "scripts": {
    "test": "jest --no-cache",
    "build": "npm run build-css && rollup -c rollup.config.js",
    "start": "npm-run-all --parallel watch-css storybook",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./src",
    "build-css": "node-sass --include-path src/styles src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/style --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jam3/public-react-ui.git"
  },
  "keywords": [
    "react",
    "react-components",
    "components",
    "jam3",
    "ui components"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jam3/public-react-ui/issues"
  },
  "homepage": "https://github.com/Jam3/public-react-ui#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "default-passive-events": "^1.0.10",
    "fullscreen-handler": "0.0.2",
    "gsap-promisify": "^1.0.2",
    "no-op": "^1.0.3",
    "prop-types": "^15.6.2",
    "react-background-video-player": "^1.1.5",
    "react-router-dom": "^4.3.1",
    "remove-trailing-separator": "^1.1.0"
  },
  "peerDependencies": {
    "gsap": ">=2.0.1",
    "react": ">=16.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/addon-info": "^3.4.7",
    "@storybook/addon-knobs": "^3.4.7",
    "@storybook/react": "^3.4.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta.6",
    "babel-preset-react-app": "^6.1.0",
    "enzyme": "^3.3.0",
    "eslint": "4.11.0",
    "eslint-config-jam3": "0.1.4",
    "eslint-config-postcss": "^2.0.2",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "^2.0.1",
    "eslint-plugin-flowtype": "2.48.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jam3": "0.2.1",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-promise": "3.7.0",
    "eslint-plugin-react": "7.5.1",
    "eslint-plugin-standard": "3.0.1",
    "gsap": ">=2.0.1",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "node-sass-chokidar": "1.3.0",
    "npm-run-all": "4.1.2",
    "prettier": "^1.11.1",
    "react": ">=16.4.1",
    "react-test-renderer": "^16.4.1",
    "rollup": "^0.60.7",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-postcss": "^1.6.2",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-svg": "^1.0.1",
    "storybook-addon-jsx": "^5.3.0"
  },
  "jest": {
    "setupFiles": [
      "./test/jest-setup.js"
    ]
  }
}
