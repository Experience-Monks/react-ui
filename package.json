{
  "name": "@jam3/react-ui",
  "version": "1.2.0",
  "description": "React components that follow Jam3 standards",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "scripts": {
    "test": "jest --no-cache",
    "build": "npm run build-css && rollup -c rollup.config.js",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./src",
    "build-storybook": "build-storybook -c .storybook -s ./src -o ./docs",
    "build-css": "node-sass --include-path src/styles src/ -o src/",
    "postinstall": "node ./src/scripts/copy-components-utility.js",
    "release": "npm run build && npm run build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jam3/react-ui.git"
  },
  "keywords": [
    "react",
    "react-components",
    "components",
    "jam3",
    "ui components"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jam3/react-ui/issues"
  },
  "homepage": "https://github.com/Jam3/react-ui#readme",
  "dependencies": {
    "@jam3/detect": "^1.0.2",
    "@jam3/react-check-extra-props": "^1.0.1",
